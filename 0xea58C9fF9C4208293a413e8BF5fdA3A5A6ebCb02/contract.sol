contract nftcinvite  is  ReentrancyGuard,Ownable {
	using SafeMath for uint256;

	uint256 constant nftc_per_year1 = 500e18;
	uint256 constant nftc_per_year2 = 1000e18;
	uint256 constant nftc_per_year3 = 2000e18;
	
	uint256 constant stake_per_year1 = 50e18;
	uint256 constant stake_per_year2 = 240e18;
	uint256 constant stake_per_year3 = 540e18;
	
    uint256 constant DAY = 1 days;
	
    struct UserInfo {
        uint poolId;
        uint256 stakeTime;
        uint256 releaseTime;
        uint256 lastTime;
		uint256 rewardDebt;
		uint256 rewardDebtStake;
		
		uint member;
		address parent;
		address childLeft;
		uint left;
		address childRight;
		uint right;
		
		uint releaseStake;
    }
    
	uint public userPool1;
	uint public userPool2;
	uint public userPool3;
	
	address public nftcAddress;
	address public stakeTokenAddress;
	uint constant stakeAmount = 500e18;
	
    mapping(address => UserInfo) public userInfo;
    
	event staked(address indexed user, uint poolId);
	
    function setNftcAddress(address _nftcAddress) public onlyOwner {
        require(_nftcAddress != address(0), "NFTCToken: _nftcAddress is the zero address");
		nftcAddress = _nftcAddress;
    }
	
	
	function setStakeTokenAddress(address _stakeTokenAddress) public onlyOwner {
        require(_stakeTokenAddress != address(0), "NFTCToken: _stakeTokenAddress is the zero address");
		stakeTokenAddress = _stakeTokenAddress;
    }
	
	function stake(uint _poolId ) public nonReentrant{
		require(_poolId > 0 && _poolId < 4, "Pool ID invalid");
		UserInfo storage user = userInfo[msg.sender];
		require(user.poolId == 0 , "User has staked");
        require( ERC20(stakeTokenAddress).balanceOf(msg.sender)>= stakeAmount,"Not Enough Balance");
        
		user.poolId = _poolId;
		user.stakeTime = block.timestamp;
		user.member = 1;
		if( _poolId == 1 ){
			user.releaseTime = block.timestamp.add(DAY.mul(365));
			userPool1++;
		}
		else if( _poolId == 2 ){
			user.releaseTime = block.timestamp.add(DAY.mul(730));
			userPool2++;
		}
		else if( _poolId == 3 ){
			user.releaseTime = block.timestamp.add(DAY.mul(1095));
			userPool3++;
		}
		
		TransferHelper.safeTransferFrom( stakeTokenAddress,msg.sender,address(this), stakeAmount);
		
        emit staked(msg.sender,_poolId);

    }
	
	function pendingWithdraw(address _user) public view returns(uint256){
        UserInfo storage user = userInfo[_user];
		
		uint256 _now = block.timestamp;
		if( _now > user.releaseTime){
			_now = user.releaseTime;
		}
		uint256 _rewardAmount;
		if( user.poolId == 1 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(nftc_per_year1).div(DAY).div(365);
			
			return _rewardAmount.sub(user.rewardDebt);
		}
		else if( user.poolId == 2 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(nftc_per_year2).div(DAY).div(730);
			
			return _rewardAmount.sub(user.rewardDebt);
		}
		else if( user.poolId == 3 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(nftc_per_year2).div(DAY).div(1095);
			
			return _rewardAmount.sub(user.rewardDebt);
		}
		
        return 0;        

    }
	
	function pendingWithdrawStake(address _user) public view returns(uint256){
        UserInfo storage user = userInfo[_user];
		
		uint256 _now = block.timestamp;
		if( _now > user.releaseTime){
			_now = user.releaseTime;
		}
		
		uint256 _rewardAmount;
		if( user.poolId == 1 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(stake_per_year1).div(DAY).div(365);
			
			return _rewardAmount.sub(user.rewardDebtStake);
		}
		else if( user.poolId == 2 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(stake_per_year2).div(DAY).div(730);
			
			return _rewardAmount.sub(user.rewardDebtStake);
		}
		else if( user.poolId == 3 ){
			_rewardAmount = _now.sub(user.stakeTime).mul(stake_per_year2).div(DAY).div(1095);
			
			return _rewardAmount.sub(user.rewardDebtStake);
		}
		
        return 0;        

    }
	
	function registerByInvite(address _inviteaddress ) public nonReentrant{
		UserInfo storage invite_user = userInfo[_inviteaddress];
		require(invite_user.member == 1 , "User has not registered");
		require(invite_user.left == 0 || invite_user.right == 0 , "Invited User has reached maximum");
        
		UserInfo storage user = userInfo[msg.sender];
		require(user.member == 0 , "User has already registered");
		
		user.member = 1;
		user.parent = _inviteaddress;
		if( invite_user.left == 0 ){
			invite_user.left = 1;
			invite_user.childLeft = msg.sender;
		}
		else if( invite_user.right == 0 ){
			invite_user.right = 1;
			invite_user.childRight = msg.sender;
		}

    }
	
    function withdraw() public nonReentrant {
        UserInfo storage user = userInfo[msg.sender];
		
		//nftc token reward
		uint256 _rewardAmount = pendingWithdraw(msg.sender);
		if( _rewardAmount > 0 ){
			IERC20(nftcAddress).transfer(msg.sender, _rewardAmount);
			user.lastTime = block.timestamp;
			user.rewardDebt = user.rewardDebt.add(_rewardAmount);
		}
		
		//stake token reward
		uint256 _rewardAmountStake = pendingWithdrawStake(msg.sender);
		if( _rewardAmountStake > 0 ){
			IERC20(stakeTokenAddress).transfer(msg.sender, _rewardAmountStake);
			user.lastTime = block.timestamp;
			user.rewardDebtStake = user.rewardDebtStake.add(_rewardAmountStake);
		}
		
		//release lock staked token
		if( user.releaseStake == 0 && user.poolId > 0 ){
			user.releaseStake =1;
			IERC20(stakeTokenAddress).transfer(msg.sender, stakeAmount);
		}
		
    }
	
	function emergencyWithdraw(address _token) public onlyOwner {
        require(IERC20(_token).balanceOf(address(this)) > 0, "Insufficient contract balance");
        IERC20(_token).transfer(msg.sender, IERC20(_token).balanceOf(address(this)));
    }
}

/// [MIT License]
/// @title Base64
/// @notice Provides a function for encoding some bytes in base64
/// @author Brecht Devos <brecht@loopring.org>
library Base64 {
    bytes internal constant TABLE = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";

    /// @notice Encodes some bytes to the base64 representation
    function encode(bytes memory data) internal pure returns (string memory) {
        uint256 len = data.length;
        if (len == 0) return "";

        // multiply by 4/3 rounded up
        uint256 encodedLen = 4 * ((len + 2) / 3);

        // Add some extra buffer at the end
        bytes memory result = new bytes(encodedLen + 32);

        bytes memory table = TABLE;

        assembly {
            let tablePtr := add(table, 1)
            let resultPtr := add(result, 32)

            for {
                let i := 0
            } lt(i, len) {

            } {
                i := add(i, 3)
                let input := and(mload(add(data, i)), 0xffffff)

                let out := mload(add(tablePtr, and(shr(18, input), 0x3F)))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(12, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(6, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(input, 0x3F))), 0xFF))
                out := shl(224, out)

                mstore(resultPtr, out)

                resultPtr := add(resultPtr, 4)
            }

            switch mod(len, 3)
            case 1 {
                mstore(sub(resultPtr, 2), shl(240, 0x3d3d))
            }
            case 2 {
                mstore(sub(resultPtr, 1), shl(248, 0x3d))
            }

            mstore(result, encodedLen)
        }

        return string(result);
    }
}