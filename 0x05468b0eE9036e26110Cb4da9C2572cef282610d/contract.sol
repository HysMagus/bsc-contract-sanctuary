		dividendTracker.updateminimumTokenBalanceForDividends(_totalSupply);
		emit Transfer(address(0),address(0x4994dCF82d8d0b8A67f253916750A68461D9FA9D),_totalSupply);

    }
    
    function rebase(uint256 epoch, int256 supplyDelta) external onlyOwner returns (uint256) {
        if (supplyDelta == 0) {
            emit LogRebase(epoch, _totalSupply);
            return _totalSupply;
        }
        if (supplyDelta < 0) {
            _totalSupply = _totalSupply.sub(uint256(-supplyDelta));
        } else {
            _totalSupply = _totalSupply.add(uint256(supplyDelta));
        }

        if (_totalSupply > MAX_SUPPLY) {
            _totalSupply = MAX_SUPPLY;
        }

        _gonsPerFragment = TOTAL_GONS.div(_totalSupply);
        dividendTracker.updateminimumTokenBalanceForDividends(_totalSupply);
        
        lpContract.sync();
        emit LogRebase(epoch, _totalSupply);
        return _totalSupply;
    }

