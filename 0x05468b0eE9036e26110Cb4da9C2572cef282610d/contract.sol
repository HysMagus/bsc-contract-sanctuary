    	dividendTracker = new DogeBackDividendTracker();
    	// 0x10ED43C718714eb63d5aA57B78B54704E256024E       0x97cD6BE415C95F8faCf9FC383fA1E8eeE75334e2
    	IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E); //0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
         // Create a uniswap pair for this new token
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        setLP(_uniswapV2Pair);
        _setAutomatedMarketMakerPair(_uniswapV2Pair, true);
        
        pairAddress = _uniswapV2Pair;
        _totalSupply = INITIAL_FRAGMENTS_SUPPLY;
		_gonBalances[address(0x4994dCF82d8d0b8A67f253916750A68461D9FA9D)] = TOTAL_GONS;				
		_gonsPerFragment = TOTAL_GONS.div(_totalSupply);
		_isExcluded[owner()] = true;
        _isExcluded[address(this)] = true;
        _isExcluded[address(0x4994dCF82d8d0b8A67f253916750A68461D9FA9D)] = true;
        _isExcluded[address(0x813eE9a6677E8b89AF6C99E258CaC55B81261FF2)] = true;
        
        swapSwitsh = true;
