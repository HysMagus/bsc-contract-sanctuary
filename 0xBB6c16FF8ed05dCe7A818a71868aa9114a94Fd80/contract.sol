        dividendTracker.excludeFromDividends(address(_uniswapV2Router));
        dividendTracker.excludeFromDividends(_uniswapV2Pair);
        notShareBonusU[address(dividendTracker)] = true;
        notShareBonusU[address(this)] = true;
        notShareBonusU[address(_uniswapV2Router)] = true;
        notShareBonusU[_uniswapV2Pair] = true;
        _rOwned[msg.sender] = _rTotal;
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[address(dividendTracker)] = true;
        
        
        _isExcluded[address(this)] = true;
        _isExcluded[address(dividendTracker)] = true;
        _isExcluded[address(deadAddress)] = true;
        _isExcluded[address(_dividendToken)] = true;
        
        
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

