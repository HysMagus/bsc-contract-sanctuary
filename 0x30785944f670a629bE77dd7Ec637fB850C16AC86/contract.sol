// File: contracts/fryworld-finance/vaults/StrategyPancakeFarm.sol
// SPDX-License-Identifier: MIT

pragma solidity ^0.6.2;

contract StrategyFryCake {
    using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint256;
    /**
     * @dev Tokens Used:
     * {output} - Token generated by staking deposits. Cake for Pancake Farming.
     * {wbnb, busd} - Required for liquidity routing when doing swaps.
     * {want} - Token that the strategy maximizes. Users Cake-LP deposit token into vault
     * {fries} - Fries token - for portion of each harvest to send to Fries stakers
     * {pancakeLPToken0, pancakeLPToken1} - Each token in the {want} LP pair. For compounding cake rewards.
     * {devAddr} - For maintenance and development fee's
    */
    address constant public wbnb = address(0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c);
    address constant public busd = address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
    address constant public output = address(0x0E09FaBB73Bd3Ade0a17ECC321fD13a19e81cE82);
    address constant public fries = address(0x393B312C01048b3ed2720bF1B090084C09e408A1);
    address public want; // underlying lp token
    address public pancakeLPToken0;
    address public pancakeLPToken1;
    address public devAddr;
    /**
     * @dev Third Party Contracts:
     * {pancakerouter} - PancakeSwap AMM used for route trading in startTheFryer()
     * {masterChef} - PancakeSwap's Main MasterChef contract. Where LP tokens are deposited and Cake is Harvested.
     */
    address public pancakeRouter = address(0x05fF2B0DB69458A0750badebc4f9e13aDd608C7F);
    address constant public masterChef = address(0x73feaa1eE314F8c655E354234017bE2193C9E24E);
    /**
     * @dev Fryworld Contracts:
     * {ketchupPool} - Reward pool where the strategy fee earnings will go.
     * {vault} - Address of the vault that controls this strategies funds.
     */
    address public ketchupPoolReward = address(0x40FAf204c3348896a5582593C3D961a435968529); // replace this.
    address public vault;
    /**
     * @dev Distribution of fees earned. This allocations relative to the % implemented
     * {withdrawalFee} - Fee taxed when a user withdraws funds. 50 === 0.5% fee.
    */
    uint public withdrawalFee = 50; // 0.5%
    uint constant public withdrawalMax = 10000;
    /**
     * @dev Swap routes on Pancake AMM
     * {swap2Token0Routing} - Route from {output} into Token0 of LP
     * {swap2Token1Routing} - Route from {output} into Token1 of LP
     * {swap2FriesRouting} - Route from {output} into Fries
     * {swap2WbnbRouting} - Route from {output} into wBNB
     */
    address[] public swap2Token0Routing; // cake to token0
    address[] public swap2Token1Routing; // cake to token1
    address[] public swap2FriesRouting; // cake to fries
    address[] public swap2WbnbRouting; // cake to wbnb

    // Display strategy name on UI for fryworld and external parties
    string public getName;
    // Should be index - pid - of the {want} pool in the pancakeswaps MasterChef contract
    uint public poolIndex;

    /**
     * @dev Initializes the strategy with the token that it will look to maximize with specified Vault.
     */
    constructor(address _want, address _vault, uint _poolIndex, address _devAddr) public {
        want = _want;
        vault = _vault;
        poolIndex = _poolIndex;
        devAddr = _devAddr;

        pancakeLPToken0 = IPancakePair(address(want)).token0();
        pancakeLPToken1 = IPancakePair(address(want)).token1();

        getName = string(
            abi.encodePacked("Fryworld:CakeStrategy: fry",
                abi.encodePacked(ERC20(want).name()," Vault"
                )
            ));

        swap2Token0Routing = [output, pancakeLPToken0];
        swap2Token1Routing = [output, pancakeLPToken1];
        swap2FriesRouting = [output, wbnb, fries];
        swap2WbnbRouting = [output, wbnb];

        IERC20(want).safeApprove(pancakeRouter, uint(-1));
        IERC20(output).safeApprove(pancakeRouter, uint(-1));
        IERC20(pancakeLPToken0).safeApprove(pancakeRouter, uint(-1));
        IERC20(pancakeLPToken1).safeApprove(pancakeRouter, uint(-1));
    }

    /**
     * @dev Function that puts the funds to work. It gets called whenever someone deposits
     * in the strategy's vault contract. It deposits the total {want} available to
     * farm CAKE in the masterChef.
     */
    function deposit() public {
        uint _want = IERC20(want).balanceOf(address(this));
        if (_want > 0) {
            IERC20(want).safeApprove(masterChef, 0);
            IERC20(want).safeApprove(masterChef, _want);
            IMasterChef(masterChef).deposit(poolIndex, _want);
        }
    }

    /**
     * @dev It withdraws funds from masterChef and sends them back to the vault.
     * Gets called when users withdraws from the vault.
     */
    function withdraw(uint _amount) external {
        require(msg.sender == vault, "!fryVault"); // only fryVault associated with strategy should call this.
        uint _balance = IERC20(want).balanceOf(address(this));

        if (_balance < _amount) {
            _amount = _withdrawCake(_amount.sub(_balance));
            _amount = _amount.add(_balance);
        }

        uint _fee = 0;
        if (withdrawalFee > 0){
            _fee = _amount.mul(withdrawalFee).div(withdrawalMax);
        }

        IERC20(want).safeTransfer(devAddr, _fee); // send 0.5% to devAddr
        uint _userAmount = IERC20(want).balanceOf(address(this));
        IERC20(want).safeTransfer(vault, _userAmount); // send remaining to vault, which sends to user.
    }

    /**
     * @dev Internal function that manages the withdrawal from Pancakes masterChef.
     */
    function _withdrawCake(uint256 _amount) internal returns (uint) {
        IMasterChef(masterChef).withdraw(poolIndex, _amount);
        return _amount;
    }

    /**
     * @dev Core function of the strategy. All the hard fryWork is done in the _liquidateReward() function
     * 1. Claims rewards from pancakeswaps masterChef
     * 2. Sends all the hard fry work off to _liquidateReward()
     * 3. Re-invests remaining LP profits into masterChef using deposit()
     */
    function startTheFryer() public {
        require(!Address.isContract(msg.sender),"!contract");
        IMasterChef(masterChef).deposit(poolIndex, 0);
        _liquidateReward();
        deposit();
    }
    /**
     * @dev Swaps {output} for pancakeLPToken1 and pancakeLPToken0 using Pancakeswap
     * Combines them into LP token
     * Sells off 18% of Cake into Fries, sends to Ketchup Pool
     * 2% of Cake sold as maintenance & gas fee
     */
    function _liquidateReward() internal {

        uint256 rewardBalance = IERC20(output).balanceOf(address(this));

        if (rewardBalance > 0)
        {
          // sell reward
          uint256 amountOutMin = 0;
          // approve reward token
          IERC20(output).safeApprove(pancakeRouter, 0);
          IERC20(output).safeApprove(pancakeRouter, rewardBalance);

          uint256 lpSplit = rewardBalance.mul(40).div(100); // 80% for LP compounding
          uint256 ketchupPool = rewardBalance.mul(18).div(100); // 18% of cake as fries to KHP holders. Remaining ~2% for gas subsidy

          // sell 40% of reward for token0
          PancakeRouter(pancakeRouter).swapExactTokensForTokens(
            lpSplit,
            amountOutMin,
            swap2Token0Routing,
            address(this),
            block.timestamp
          );
          uint256 token0Amount = IERC20(pancakeLPToken0).balanceOf(address(this));

          // sell 40% of reward for token1
          PancakeRouter(pancakeRouter).swapExactTokensForTokens(
            lpSplit,
            amountOutMin,
            swap2Token1Routing,
            address(this),
            block.timestamp
          );
          uint256 token1Amount = IERC20(pancakeLPToken1).balanceOf(address(this));

          // send token1 & token2 to pancakeLP
          IERC20(pancakeLPToken0).safeApprove(pancakeRouter, 0);
          IERC20(pancakeLPToken0).safeApprove(pancakeRouter, token0Amount);

          IERC20(pancakeLPToken1).safeApprove(pancakeRouter, 0);
          IERC20(pancakeLPToken1).safeApprove(pancakeRouter, token1Amount);

          uint256 liquidity;
          (,,liquidity) = PancakeRouter(pancakeRouter).addLiquidity(
            pancakeLPToken0,
            pancakeLPToken1,
            token0Amount,
            token1Amount,
            1,
            1,
            address(this),
            block.timestamp
          );

          // sell 18% of reward for fries
          PancakeRouter(pancakeRouter).swapExactTokensForTokens(
            ketchupPool,
            amountOutMin,
            swap2FriesRouting,
            address(this),
            block.timestamp
          );
          uint256 friesAmount = IERC20(fries).balanceOf(address(this));

          // send fries to KetchupPool
          IERC20(fries).safeTransfer(ketchupPoolReward, friesAmount);

          uint256 remaining = IERC20(output).balanceOf(address(this));
          // sell ~2% of reward for maintenance fee
          PancakeRouter(pancakeRouter).swapExactTokensForTokens(
            remaining,
            amountOutMin,
            swap2WbnbRouting,
            address(this),
            block.timestamp
          );

          // send to devAddr -> gov
          uint256 remainingWork = IERC20(wbnb).balanceOf(address(this));
          IERC20(wbnb).safeTransfer(devAddr, remainingWork);
        }
    }
    /**
     * @dev Function to calculate the total underlying asset {want} held by the strategy.
     * It takes into account both the funds in hand, as the funds allocated in masterChef
     */
    function balanceOf() public view returns (uint) {
        return balanceOfWant()
               .add(balanceOfPool());
    }

    /**
     * @dev It calculates how much {want} the contract holds.
     */
    function balanceOfWant() public view returns (uint) {
        return IERC20(want).balanceOf(address(this));
    }

    /**
     * @dev It calculates how much {want} the strategy has allocated in cake's masterChef
     */
    function balanceOfPool() public view returns (uint) {
        (uint _amount, ) = IMasterChef(masterChef).userInfo(poolIndex, address(this));
        return _amount;
    }
}